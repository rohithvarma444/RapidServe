pipeline {
    agent any

    environment {
        DOCKER_CREDENTIALS = credentials('rohithvarma- Dockerhub')
    }

    stages {
        stage('Docker Login'){
            steps{
                sh 'echo "Login to Docker Hub"'
                sh '''
                    echo "Docker Credentials:"
                    echo "Username: ${DOCKER_CREDENTIALS_USR}"
                    echo "Password: ${DOCKER_CREDENTIALS_PSW}"
                '''
            }
        }
    }
}